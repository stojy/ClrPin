<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:scanner="clr-namespace:ClrVpin.Scanner">


  <DataTemplate x:Key="ScannerStatisticsTemplate" DataType="{x:Type scanner:ScannerStatistics}">
    <DockPanel>
      <TextBox Text="{Binding Result}" FontFamily="Courier New"></TextBox>

    </DockPanel>
  </DataTemplate>

</ResourceDictionary>