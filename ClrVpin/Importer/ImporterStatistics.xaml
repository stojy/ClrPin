<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:importer="clr-namespace:ClrVpin.Importer">

  <DataTemplate x:Key="ImporterStatisticsTemplate" DataType="{x:Type importer:ImporterStatisticsViewModel}">
    <DockPanel>
      <TextBox Margin="5" Text="{Binding Result}" FontFamily="Courier New" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0"/>
    </DockPanel>
  </DataTemplate>

</ResourceDictionary>