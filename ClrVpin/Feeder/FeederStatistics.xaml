<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:feeder="clr-namespace:ClrVpin.Feeder">

  <DataTemplate x:Key="FeederStatisticsTemplate" DataType="{x:Type feeder:FeederStatisticsViewModel}">
    <DockPanel>
      <TextBox Margin="5" Text="{Binding Result}" FontFamily="Courier New" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0" VerticalAlignment="Top"/>
    </DockPanel>
  </DataTemplate>

</ResourceDictionary>